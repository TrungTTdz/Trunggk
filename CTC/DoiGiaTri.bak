program DoiGiaTri;
uses crt;
var
        x,y:integer;

procedure Swap(var x,y:integer);
var
        t:integer;

BEGIN
        t:=a; a:=b; b:=t;
        write('Gia tri cua x =',a,' y=',b);

END;
BEGIN
        clrscr;

        write('Nhap x,y:');readln(x,y);
        writeln('Truoc khi doi x =',x,' y= ',y);
        Swap();


        readln
END.